<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>


<VBox xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.esprit.outdoors.controllers.PublicationResultController">
   <children>
      <Label fx:id="titre" prefHeight="30.0" prefWidth="150.0" text="Titre" />
      <TextArea fx:id="text" editable="false" prefHeight="97.0" prefWidth="558.0" />
      <MenuButton mnemonicParsing="false" prefHeight="25.0" prefWidth="32.0">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#modfier" text="Modifier" />
          <MenuItem mnemonicParsing="false" onAction="#supprimer" text="Supprimer" />
            <MenuItem mnemonicParsing="false" onAction="#signaler" text="Signaler" />
        </items>
      </MenuButton>
      <Button mnemonicParsing="false" onAction="#afficherCommentaire" prefHeight="30.0" prefWidth="161.0" text="Afficher les commentaires" />
      <Label fx:id="nbrCom" alignment="CENTER_RIGHT" prefHeight="30.0" prefWidth="45.0" text="0" />
      <Label fx:id="date" prefHeight="30.0" prefWidth="102.0" text="Date" />
      <ImageView fx:id="imgposter" fitHeight="45.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</VBox>
